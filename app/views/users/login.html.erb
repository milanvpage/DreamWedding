<h1>Log In</h1>

<h2>
<%= form_with(url: users_path) do |f| %>
    <%= f.label :business_name %>
    <%= f.text_field :business_name %>
<br>
    <%= f.label :email %>
    <%= f.email_field :email %>
<br>   
    <%= f.label :phone_number %>
    <%= f.number_field :phone_number %>
<br>
    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= submit_tag "Log In" %>
<% end %>
</h2>