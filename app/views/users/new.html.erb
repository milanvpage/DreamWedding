<h1>Sign Up</h1>
#url: '/signup'
<%= form_for(@user) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email, required:true %>
    <br>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <br>
    <br>
    <br>
    

    <%= f.submit "sumbit" %>

<% end %>