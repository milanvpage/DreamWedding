<h1>Sign Up</h1>
<h2>
<%= form_for(@user, url: signup_path) do |f| %>
    <%= f.label :business_name %>
    <%= f.text_field :business_name %>
    <br>
    <%= f.label :email %>
    <%= f.email_field :email, required:true %>
    <br>
    <%= f.label :phone_number %>
    <%= f.number_field :phone_number %>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %>
    

    <%= f.submit "Sign Up" %>

<% end %>
</h2>