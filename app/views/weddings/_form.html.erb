<%= render partial: "layouts/error", locals: { object: @wedding } %>

<%= form_for([@venue, @wedding]) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :color_scheme %>
    <%= f.text_field :color_scheme %>
    <%= f.label :entertainment %>
    <%= f.text_field :entertainment %>
    <%= f.label :flowers %>
    <%= f.text_field :flowers %>
    <%= venue_form_helper(f) %>
    <%= f.submit %>
<% end %>
